package thamarai2006;

public class d {

	public static void main(String[] args) {
	
		int [][]a={{1,2,3,4},
				{5,6,7,8},
				{9,2,5,6},
				{3,4,8,7}};
		
					
		int max=0;
		for(int[] row:a){
	
			for(int num : row ){
				if(num>max){
					max=num;
				}
			}
		}
		System.out.println("largest Element:"+max);
	
}
}		
				 
			

