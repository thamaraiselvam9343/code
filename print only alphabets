package thamarai2006;

public class word {

	public static void main(String[] args) {
		 String a = "sdju4328hjrgshjsab";
		 String b="";
	       

	        for (int i = 0; i <a.length(); i++) {
	            if ('a'<a.charAt(i) || 'z'<a.charAt(i)){
	            	b=b+(a.charAt(i));
	            }
	           
	        }

	        System.out.println(b);

	}

}
